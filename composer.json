{
  "name": "astgroup/laravel-amazon-mws",
  "type": "library",
  "description": "A minimal service provider to set up and use the MWS API PHP library in Laravel 5.*",
  "license": "GPL-3.0-only",
  "keywords": [
    "API",
    "Amazon",
    "PHP",
    "MWS",
    "Laravel",
    "Persistent",
    "Config",
    "Amazon-api",
    "FBA"
  ],
  "authors": [
    {
      "name": "ZENKR",
      "email": "zenkr@qq.com"
    }
  ],
  "require": {
    "laravel/framework": "5.6.*"
  },
  "autoload": {
    "psr-4": {
      "Mws\\Laravel\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "LaravelAmazonMws\\MwsServiceProvider"
      ]
    }
  }
}